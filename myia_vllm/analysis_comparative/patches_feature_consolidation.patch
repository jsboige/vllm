+    }
+}
+
+function Get-ContainerLogs {
+    param (
+        [string]$ContainerName,
+        [string]$ServiceName
+    )
+    
+    Write-Log "INFO" "RÃ©cupÃ©ration des logs..." $ServiceName
+    
+    if (-not (Test-ContainerStatus $ContainerName)) {
+        Write-Log "ERROR" "Conteneur non trouvÃ© ou arrÃªtÃ©: $ContainerName" $ServiceName
+        return
+    }
+    
+    $dockerArgs = @("logs")
+    
+    if ($Follow) {
+        $dockerArgs += "-f"
+    }
+    
+    $dockerArgs += "--tail", $TailLines.ToString()
+    $dockerArgs += $ContainerName
+    
+    try {
+        if ($Follow) {
+            Write-Log "INFO" "Mode suivi activÃ© (Ctrl+C pour arrÃªter)" $ServiceName
+            & docker $dockerArgs | ForEach-Object {
+                Format-LogLine $_ $ServiceName
+            }
+        } else {
+            $logs = & docker $dockerArgs 2>&1
+            
+            if ($LASTEXITCODE -eq 0) {
+                $logs | ForEach-Object {
