+#!/usr/bin/env python3
+# -*- coding: utf-8 -*-
+
+"""
+Client pour interagir avec l'API OpenAI de vLLM.
+"""
+
+import json
+from typing import Dict, Optional
+
+import requests
+
+from .utils import log
+
+class VLLMClient:
+    """Un client simple pour l'API vLLM."""
+
+    def __init__(self, endpoint: str, api_key: Optional[str] = None):
+        """
+        Initialise le client.
+        
+        Args:
+            endpoint: URL de base de l'API (ex: http://localhost:5000/v1)
+            api_key: ClÃ© API pour l'authentification (optionnelle)
+        """
+        self.endpoint = endpoint
+        self.api_key = api_key
+        self.headers = {
+            "Content-Type": "application/json"
+        }
+        if self.api_key:
+            self.headers["Authorization"] = f"Bearer {self.api_key}"
+
+    def _send_request(self, method: str, path: str, **kwargs) -> requests.Response:
+        """
+        MÃ©thode interne pour envoyer des requÃªtes.
+        
+        Args:
+            method: MÃ©thode HTTP (GET, POST, etc.)
+            path: Chemin de l'API (ex: /models)
+            **kwargs: Arguments supplÃ©mentaires pour requests
+        
+        Returns:
+            requests.Response: La rÃ©ponse de la requÃªte
+        
+        Raises:
+            requests.exceptions.RequestException: En cas d'erreur de requÃªte
+        """
+        url = f"{self.endpoint}{path}"
+        log("DEBUG", f"Envoi de la requÃªte {method} Ã  {url}...")
+        log("DEBUG", f"DonnÃ©es: {json.dumps(kwargs.get('json', {}), indent=2, ensure_ascii=False)}")
+        
+        response = requests.request(
+            method,
+            url,
+            headers=self.headers,
+            timeout=30,
+            **kwargs
+        )
+        response.raise_for_status()  # LÃ¨ve une exception pour les codes d'erreur HTTP
+        return response
+
+    def get_models(self) -> Dict:
+        """RÃ©cupÃ¨re la liste des modÃ¨les disponibles."""
+        log("INFO", "RÃ©cupÃ©ration de la liste des modÃ¨les...")
+        response = self._send_request("GET", "/models")
+        return response.json()
+
+    def chat_completion(self, **kwargs) -> Dict:
+        """
+        Effectue une requÃªte de chat completion.
+        
+        Args:
+            **kwargs: Arguments pour la requÃªte de chat completion (model, messages, etc.)
+        
+        Returns:
+            Dict: La rÃ©ponse JSON de l'API
+        """
+        log("INFO", "Envoi d'une requÃªte de chat completion...")
+        response = self._send_request("POST", "/chat/completions", json=kwargs)
+        return response.json()
+
+    def stream_chat_completion(self, **kwargs) -> requests.Response:
+        """
+        Effectue une requÃªte de chat completion en streaming.
+        
+        Args:
+            **kwargs: Arguments pour la requÃªte de chat completion (model, messages, etc.)
+        
+        Returns:
+            requests.Response: La rÃ©ponse en streaming
+        """
+        log("INFO", "Envoi d'une requÃªte de chat completion en streaming...")
+        kwargs["stream"] = True
+        return self._send_request("POST", "/chat/completions", json=kwargs, stream=True)
