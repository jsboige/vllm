+        
+        if (Test-ContainerStatus $containerName) {
+            $status = "âœ… ACTIF"
+            $color = $GREEN
+        } else {
+            $status = "âŒ ARRÃŠTÃ‰"
+            $color = $RED
+        }
+        
+        Write-Host -ForegroundColor $color "  $serviceName ($($service.description)): $status"
+        
+        if ($Verbose) {
+            try {
+                $containerInfo = docker inspect $containerName --format "{{.State.Status}} - DÃ©marrÃ©: {{.State.StartedAt}}" 2>$null
+                if ($containerInfo) {
+                    Write-Host -ForegroundColor $BLUE "    DÃ©tails: $containerInfo"
+                }
+            } catch {
+                # Ignorer les erreurs de dÃ©tail
+            }
+        }
+    }
+    Write-Log "INFO" ""
+}
+
+function Monitor-Services {
+    param (
+        [array]$ServiceNames
+    )
+    
+    foreach ($serviceName in $ServiceNames) {
+        $service = $SERVICES[$serviceName]
+        $containerName = $service.container_name
+        
+        Write-Log "INFO" "=== MONITORING: $serviceName ($($service.description)) ==="
+        
+        if ($ServiceNames.Count -gt 1 -and -not $Follow) {
+            Write-Log "INFO" "Conteneur: $containerName"
+            Write-Log "INFO" "DerniÃ¨res $TailLines lignes:"
+            Write-Host ""
+        }
+        
+        Get-ContainerLogs $containerName $serviceName
+        
+        if ($ServiceNames.Count -gt 1 -and -not $Follow) {
+            Write-Host ""
+            Write-Log "INFO" "--- Fin des logs pour $serviceName ---"
