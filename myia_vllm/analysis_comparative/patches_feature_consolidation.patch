+    }
+}
+
+# Patterns critiques Ã  surveiller
+$CRITICAL_PATTERNS = @(
+    "ERROR",
+    "CRITICAL",
+    "FATAL",
+    "Exception",
+    "Traceback",
+    "OutOfMemoryError",
+    "CUDA out of memory",
