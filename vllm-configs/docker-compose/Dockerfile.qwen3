FROM vllm/vllm-openai:latest

# Copier les fichiers du parser Qwen3
COPY build/tool_parsers/qwen3_tool_parser.py /vllm/vllm/entrypoints/openai/tool_parsers/
COPY build/tool_parsers/__init__.py /vllm/vllm/entrypoints/openai/tool_parsers/

# Copier le fichier du parser de raisonnement Qwen3 corrigé (PR #17506)
COPY build/reasoning/qwen3_reasoning_parser.py /vllm/vllm/reasoning/

# Définir le répertoire de travail
WORKDIR /vllm